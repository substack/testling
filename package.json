{
    "name": "testling",
    "description": "write tests for browser code",
    "version": "1.7.3",
    "repository": {
        "type": "git",
        "url": "git://github.com/substack/testling.git"
    },
    "browserify": "browser.js",
    "bin": {
        "testling": "bin/cmd.js"
    },
    "keywords": [
        "test",
        "browser",
        "headless"
    ],
    "scripts": {
        "prebuild": "rimraf bundle",
        "build": "mkdir -p bundle && browserify browser/prelude.js -o bundle/prelude.js",
        "prepublish": "safe-publish-latest && npm run build"
    },
    "dependencies": {
        "bouncy": "~3.2.2",
        "browser-launcher": "~1.0.1",
        "browserify": "3.x.x",
        "concat-stream": "~1.0.1",
        "ecstatic": "~4.1.2",
        "ent": "~0.0.7",
        "glob": "~3.2.11",
        "jsonify": "~0.0.0",
        "object-inspect": "~1.7.0",
        "optimist": "~0.5.2",
        "resolve": "~1.14.2",
        "shallow-copy": "~0.0.1",
        "shell-quote": "~1.7.2",
        "tap-finished": "~0.0.1",
        "win-spawn": "~2.0.0",
        "xhr-write-stream": "~0.1.2"
    },
    "devDependencies": {
        "rimraf": "~3.0.0",
        "safe-publish-latest": "~1.1.4",
        "tape": "~5.0.0-next.3"
    },
    "engines": {
        "node": ">=0.6"
    },
    "license": "MIT/X11",
    "author": {
        "name": "James Halliday",
        "email": "mail@substack.net",
        "url": "http://substack.net"
    }
}
